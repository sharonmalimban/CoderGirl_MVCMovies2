
@{
    ViewData["Title"] = "Movies";
}

<h1>Movies</h1>
<a asp-controller="Movie" asp-action="Create">Add New Movie</a>
<table>
    <tr>
        <th>Movie Name</th>
    </tr>
    @foreach (var movie in ViewBag.Movies)
    {
        <tr>
            <td>@movie.Value</td>
        </tr>
    }
</table>
